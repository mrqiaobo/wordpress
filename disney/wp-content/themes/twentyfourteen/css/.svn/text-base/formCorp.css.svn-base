/*---Mydisneyland---*/

.BoxHead{
	margin-left:10px;
	color: #002B6D;
}
.BoxHeadTxt{
	text-align: left;
	float: left;
	width: 295px;
	padding-left: 5px;
}
.BoxHeadRight{
	width: 136px;
	float: left;
	text-align: right;
	padding-right: 4px;
}

.TitleOrange{
	color: #F47508;
	font-size: 14px;
	font-weight: bold;
}

/*---Mydisneyland reasons---*/

.reason{
	width: auto;
	height: auto;
	text-align: left;
}

.reasonLeft{
	width: 46px;
	float: left;
}

.reasonRight{
	width: 430px;
	float: left;
}

/*---Mydisneyland sign-in---*/

.signInBox{
	width: 497px;
	text-align: left;
}

.signInBoxCenter .OblFields{
	padding-left: 47px;
	color: #002B6D;
}

#signInTab{
	width: 475px;
	margin: 0px 10px;
	color: #002B6D;
}

.Warning{
	width:160px;
	height: auto;
	font-size: 9px;
	font-weight: bold;
	color: #c92702;
	float: left;
}

.WarningTop,
.WarningBottom{
	background-position: 0 0;
	background-repeat: no-repeat;
	height: 6px;
	font-size: 0px;
}


.WarningCenter{
	background-color: #FFFDF1;
	padding: 5px 0px;
}


.WarningTxt{
	float: left;
	width:124px;
}

.LinkForgot{
	background-color: #FFFFFB;
	margin: 0px 3px 0px 1px;
}

/*---Mydisneyland Not registered---*/

.RegistrationBox{
	width: 497px;
	text-align: left;
}

.Rsltc1 {
	width:210px;
	float:left;
}
.Rsltc2 {
	float:left;
}

.bold{
	font-weight: bold;
}


.checkbox{
	border: none !important;
}

.error,.errorC2{
	color: #C92702 !important;
	font-weight: bold;
	float: left;
	text-align: left;
}


#error{
	text-align: center;
	width: 80%;
}

