/*---Formulaires---*/

Form{
	text-align: left;
	padding-left:10px;	
}

Form1{
	text-align: left;
}
.FormR1{ width: 100%; height: auto;}
.FormC1{ float: left; width: 240px; padding-left: 10px; }
.FormC2{ float: right; width: 230px; padding-left: 10px; }

label{
	font-weight: bold;
}

.normal{
	font-weight: normal;
}

Form input, Form1 input,
Form select, Form1 select,
Form textarea, Form1 textarea{
	font-size: 10px;
	font-weight: bold;
	margin-right: 8px;
}

h3.FormSubtitle{
	font-size: 14px;
	padding:5px 0px 0px 10px;
	color:#00267F;
}

h3.FormSubtitle1{
	font-size: 11px;
	color:#00267F;
}

.OblFieldsR,
.OblFields{
