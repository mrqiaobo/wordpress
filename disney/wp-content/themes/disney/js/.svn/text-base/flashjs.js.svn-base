function toggleDisplay(me) 
{ 
  oObj = document.getElementById(me); 
  if (oObj) 
  { 
    if (oObj.style.display == "block" || oObj.style.display =='') 
      oObj.style.display="none"; 
    else 
        oObj.style.display="block"; 
  } 
} 

